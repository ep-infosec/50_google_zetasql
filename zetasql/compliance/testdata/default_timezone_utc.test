[default_time_zone=Etc/UTC]
==
[name=cast_timestamp_to_date]

SELECT CAST(ts AS DATE) FROM (
  SELECT TIMESTAMP '2000-02-03 04:05:06+00' ts UNION ALL
  SELECT TIMESTAMP '1970-01-01 23:59:59+00' ts UNION ALL
  SELECT TIMESTAMP '1970-01-01 00:00:01+00' ts UNION ALL
  SELECT TIMESTAMP '1970-01-01 00:00:00+00' ts UNION ALL
  SELECT TIMESTAMP '1969-12-31 23:59:59+00' ts UNION ALL
  SELECT TIMESTAMP '1969-12-31 00:00:01+00' ts UNION ALL
  SELECT TIMESTAMP '1969-12-31 00:00:00+00' ts UNION ALL
  SELECT TIMESTAMP '1969-12-30 23:59:59+00' ts UNION ALL
  SELECT TIMESTAMP '1234-10-20 20:30:40+00' ts
) ORDER BY ts DESC
--
ARRAY<STRUCT<DATE>>[
  {2000-02-03},
  {1970-01-01},
  {1970-01-01},
  {1970-01-01},
  {1969-12-31},
  {1969-12-31},
  {1969-12-31},
  {1969-12-30},
  {1234-10-20}
]
