CREATE TABLE AnalyticTableWithoutSpecialValues AS
SELECT CAST(1 AS INT64) as row_id, CAST(10 AS BIGNUMERIC) as bignumeric_val UNION ALL
  SELECT 2, 11 UNION ALL
  SELECT 3, 11 UNION ALL
  SELECT 4, 12 UNION ALL
